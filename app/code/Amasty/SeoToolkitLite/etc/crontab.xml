<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="amseotoolkit_delete_expired_redirects"
             instance="Amasty\SeoToolkitLite\Cron\Redirects\DeleteExpired"
             method="execute"
        >
            <schedule>1 * * * *</schedule>
        </job>
    </group>
</config>
